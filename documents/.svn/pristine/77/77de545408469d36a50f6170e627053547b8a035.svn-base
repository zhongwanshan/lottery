
.h-50 {
    height: 50px;
}

.mar_t10 {
    margin-top: 10px;
}

.mar_L10 {
    margin-left: 10px;
}

.line-50 {
    line-height: 50px
}

.ver_align_top {
    vertical-align: top;
}

.bg_red {
    background-color: red;
}

.bg_gray {
    background-color: rgba(81, 81, 81, 1);
}

.bg_yellow {
    background-color: rgba(255, 102, 0, 1);
}

.bg_blue {
    background-color: rgba(29, 158, 215, 1);
}

.min-height {
    height: calc(100% - 110px);

}

/*订单详情 图标*/
.order_info_img {
    width: 36px;
    height: 36px;
    cursor: pointer;
}

.overflow {
    overflow: hidden;
    position: relative;
}

/*售后申请 列表的样式*/
.aftersale_list {
    margin: 10px;
    padding-bottom: 1px;
}

.aftersale_list .list_order {
    border-radius: 10px;
    background-color: white;
    border: 1px solid #E6E6E6;
    margin-bottom: 10px;
}

.aftersale_list .list_order table {
    width: 100%;
}

.aftersale_list .Bold_12 {
    font-weight: 700;
    font-style: normal;
    font-size: 12px;
    color: rgb(102, 102, 102);
    margin-right: 10px;
}

.aftersale_list .list_order .order_top {
    height: 44px;
    line-height: 44px;
    background-color: rgba(245, 245, 245, 1);
    box-sizing: border-box;
    border-bottom: 1px solid rgba(228, 228, 228, 1);
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}

.aftersale_list .list_order .order_top ul {
    margin-bottom: 0 !important;
    padding: 0 10px;
}

.aftersale_list .list_order .order_top ul li {
    float: left;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #000000;
}

.aftersale_list .tbody_ul {
    margin-bottom: 0 !important;
    padding: 0 10px;
}

.aftersale_list .tbody_ul > li {
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #000000;
    min-height: 34px;
    line-height: 34px;
}

.aftersale_list .tbody_state_left {
    display: inline-block;
    vertical-align: top;
}

.aftersale_list .tbody_state_right {
    display: inline-block;
}

.aftersale_list .tbody_reason {
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #000000;
    min-height: 22px;
    line-height: 22px;
    margin-top: 5px;
}

.aftersale_list .order_bottom {
    height: 50px;
    border-top: 1px solid #e8e4e4;
    padding: 0 10px;
}

.aftersale_list .design_name_text {
    font-weight: 400;
    font-style: normal;
    font-size: 14px;
    color: #008000;
    line-height: 50px;
}

.old_order_btn, .chat_btn, .assess_btn {
    padding: 0 20px;
    height: 30px;
    background-color: white;
    border-radius: 30px;
    outline: none;
    margin-top: 10px;
    margin-left: 10px;
}

.old_order_btn {
    border: 1px solid #008000;
    color: #008000;
}

.chat_btn {
    border: 1px solid rgb(255, 102, 0);
    color: rgb(255, 102, 0);
}

.assess_btn {
    border: 1px solid #7878ff;
    color: #7878ff;
}

.old_order_btn img, .chat_btn img, .assess_btn img {
    width: 24px;
    height: 24px;
    margin-right: 5px;
}

/*今日订单*/
.today_new {
    height: 40px;
    line-height: 40px;
    padding: 0 10px;
    border-bottom: 1px dashed rgb(228, 228, 228);
}

.today_new > img {
    width: 18px;
    height: 18px;
    margin-right: 5px;
}

.today_new .new_text {
    font-weight: 400;
    font-style: normal;
    font-size: 14px;
}

.today_new .new_num {
    font-weight: 700;
    font-style: normal;
    font-size: 14px;
    text-align: right;
}

.today_line {
    background: url(../../images/order/underline2.png);
    height: 2px;
}

.today_time {
    padding: 0 10px;
    height: 36px;
    line-height: 36px;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #BCBCBC;
}

/*今日订单的样式*/
.today_order {
    /*height: 204px;*/
    background: inherit;
    background-color: rgba(255, 255, 255, 1);
    box-sizing: border-box;
    border: 1px solid rgba(0, 153, 102, 1);
    border-radius: 8px;
    margin: 0 10px 10px 10px;
}

.today_order .order_top {
    height: 40px;
    background: inherit;
    background-color: rgba(29, 158, 215, 1);
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
}

.today_order .order_top .top_div1 {
    font-weight: 700;
    font-style: normal;
    font-size: 12px;
    color: #FFFFFF;
    width: 65%;
    line-height: 38px;
    padding-left: 10px;
}

.today_order .order_top .top_div1 > span {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    width: 180px;
    vertical-align: middle;
}

.today_order .order_top .top_div2 {
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #FFFFFF;
    width: 35%;
    padding-right: 10px;
}

.today_order .order_top .top_div2 ul {
    margin-bottom: 0 !important;
}

.today_order .order_top .top_div2 ul li {
    line-height: 20px;
    height: 20px;
    font-size: 12px;
}

.today_order .order_top .top_div3 {
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #FFFFFF;
    width: 35%;
    padding-right: 10px;
    line-height: 40px;
    text-align: right;
}

.today_order .order_content {
}

.today_order .order_content .ul_1 {
    padding: 0 10px;
    margin-bottom: 0 !important;
    overflow: hidden;
    width: 100%;
}

.today_order .order_content .ul_1 li {
    line-height: 28px;
    width: 100%;
    float: left;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #949494;
}

.today_order .order_content .ul_1 li > span {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    /* width: 152px;*/
    width: calc(100%);
    vertical-align: middle;
}

.today_order .order_content .ul_2 {
    padding: 0 10px;
    margin-bottom: 0 !important;
    overflow: hidden;
}

.today_order .order_content .font-black > li {
    color: black !important;
}
.today_order .order_content .ul_2 li {
    line-height: 28px;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #949494;
}

.today_order .order_content .ul_2 li > span {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    /* width: 220px;*/
    width: calc(100%);
    vertical-align: middle;
}

.today_order .order_content1 {
    text-align: center;
    position: relative;
    padding-right: 10px;
}

.today_order .order_content1_img {
    text-align: center;
    position: relative;
    padding-right: 100px;
}

.today_order .order_content1_type {
    position: absolute;
    right: 25%;
    top: 10px;
    color: #e92929;
    padding: 1px 3px;
    border: 1px solid #e92929;
}

.today_order .order_content1 > span {
    display: block;
    color: #009966;
    font-size: 12px;
    font-weight: 400;
    overflow: hidden;
    text-overflow: ellipsis;
}

.today_order .order_operation {
    overflow: hidden;
    border-top: 1px solid #e6e5e5;
}

.order_operation_span_left {
    font-size: 14px;
    color: #008000;
    float: left;
    margin-left: 5px;
    margin-top: 5px;
}

.order_operation_span_right {
    font-size: 14px;
    color: #008000;
    float: right;
    margin-right: 5px;
    margin-top: 5px;
}
.today_order .order_operation ul {
    padding: 0 10px;
    margin-bottom: 0 !important;
}

.today_order .order_operation ul li {
    float: left;
    width: 50%;
    line-height: 38px;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: #009966;
    cursor: pointer;
}

.today_order .order_operation ul li i {
    margin-right: 3px;
}

.today_order .order_info_btn {
    display: block;
    border-radius: 10px;
    padding: 5px 10px;
    cursor: pointer;
}

.case_div {
    min-width: 1250px;
    margin: 10px 15px;
    border: 1px solid rgba(242, 242, 242, 1);
    overflow: hidden;
    min-height: calc(100% - 150px);
}


.list_width {
    width: calc(100%);
}

.list_width .list_top_div {
    height: 50px;
    padding: 0 15px;
    border-bottom: 1px solid rgba(242, 242, 242, 1);
}

.list_width .list_top_div .type_span {
    line-height: 50px;
}

.today_div {
    width: calc(33.3%);
}

.point_case_div{
    width: 50%;
}


.case_div .type_btn {
    padding: 2px 10px;
    border-radius: 30px;
    font-weight: 400;
    font-style: normal;
    font-size: 12px;
    color: rgb(150, 142, 142);
    margin-left: 10px;
    outline: none;
    border: 1px solid #FF6600;
    background-color: white;
}

.case_div .type_active {
    background-color: #FF6600 !important;
    color: white;
}

/*加工管理订单  3等分*/
.platform_list {
    width: calc(33.3% - 20px);
    float: left;
    box-shadow: rgba(148, 148, 148, 0.35) 5px 5px 5px;
    border-radius: 4px;
    border: 1px solid rgb(228, 228, 228);
    margin: 10px;
    min-height: 90%;
}

.platform_div {
    width: 100%;
}

/*point 订单 2等分*/
.point_list {
    width: calc(50% - 20px);
    float: left;
    box-shadow: rgba(148, 148, 148, 0.35) 5px 5px 5px;
    border-radius: 4px;
    border: 1px solid rgb(228, 228, 228);
    margin: 10px;
    min-height: 600px;
}

.point_div {
    width: 100%;
}

/*日历样式*/
.Mydatepicker-here {
    width: 124px;
    height: 32px;
    line-height: 32px;
    background-color: rgba(255, 255, 255, 1) !important;
    border: 1px solid rgba(240, 130, 0, 1);
    outline: none;
    z-index: 9;
    background: url(../../images/order/date.png) no-repeat;
    background-size: 20px 20px;
    background-position: 98px 5px;
    padding-left: 10px;
    padding-right: 30px;
    border-radius: 4px;
}

.order_search_one {
    width: 32px;
    height: 32px;
    color: #8a8a8a !important;
    background-color: #fff !important;
    border-color: rgba(240, 130, 0, 1) !important;
    border-radius: 5px;
}

.order_search_one > i {
    margin-left: -3px;
}