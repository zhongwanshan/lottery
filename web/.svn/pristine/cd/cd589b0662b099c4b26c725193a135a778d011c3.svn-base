.circle {
  position: relative;
  z-index: 0;
  display: inline-block;
  border-radius: 50%;
  width: 30px;
  height: 30px;
  line-height: 30px;
  margin: 0;
  color: #e51f42;
  text-align: center;
  font-size: 14px;
  cursor: pointer;
  overflow: hidden;
  background: radial-gradient(circle at 50% 120%, #D2D2D2, #EFEFEF 10%, #ECEAEA 80%, #D0CECE 100%);
  margin-right: 5px;
}
.circle input {
  position: absolute;
  z-index: 10;
  border-radius: 50%;
  width: 30px;
  height: 30px;
  line-height: 30px;
  opacity: 0;
  left: 0;
  cursor: pointer;
}
.circle.blueBall {
  color: #695BFA;
}
.circle.active.redBall {
  color: white;
  background: radial-gradient(circle at 50% 120%, #F99D9D, #E69898 10%, #EA4F4F 80%, #E8A2A2 100%);
}
