.circle {
  position: relative;
  z-index: 0;
  display: inline-block;
  border-radius: 50%;
  width: 1.4rem;
  height: 1.4rem;
  line-height: 1.4rem;
  margin: 0;
  color: #e51f42;
  text-align: center;
  font-size: 14px;
  overflow: hidden;
  background: radial-gradient(circle at 50% 120%, #D2D2D2, #EFEFEF 10%, #ECEAEA 80%, #D0CECE 100%);
}
.circle input {
  position: absolute;
  z-index: 10;
  border-radius: 50%;
  width: 1.4rem;
  height: 1.4rem;
  line-height: 1.4rem;
  opacity: 0;
  left: 0;
}
.circle.redBall {
  margin-right: .25rem;
