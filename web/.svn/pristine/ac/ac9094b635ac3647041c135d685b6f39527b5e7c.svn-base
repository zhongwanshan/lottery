html {
  font-size: 20px;
}
::-webkit-scrollbar {
  width: 0px;
}
body {
  font-family: "Helvetica Neue", Helvetica, "微软雅黑", STHeiTi, sans-serif;
}
table {
  border-collapse: collapse;
}
.navIcon {
  display: inline-block;
  width: 1.15rem;
  height: 1.5rem;
}
.navIcon.navIcon-home {
  background: url(../img/icon/home_off.png) no-repeat center center;
  background-size: 100%;
}
.navIcon.navIcon-hall {
  background: url(../img/icon/hall_off.png) no-repeat center center;
  background-size: 100%;
}
.navIcon.navIcon-prize {
  width: 1.1rem;
  background: url(../img/icon/prize_off.png) no-repeat center center;
  background-size: 100%;
}
.navIcon.navIcon-magic {
  width: 1.25rem;
  background: url(../img/icon/magic_off.png) no-repeat center center;
  background-size: 100%;
}
.navIcon.navIcon-me {
  background: url(../img/icon/me_off.png) no-repeat center center;
  background-size: 100%;
}
.navIcon.navIcon-message {
  background: url(../img/icon/message.png) no-repeat center center;
  background-size: 100%;
}
.toolbar {
  font-size: 1rem;
}
.navbar {
  background-color: #6F00BD;
  color: white;
}
.navbar .right a {
  font-size: 14px;
  text-align: center;
}
.navbar a {
  display: inline-block;
  width: 2rem;
  height: 2.2rem;
  line-height: 2.2rem;
  vertical-align: middle;
  color: white;
}
.navbar a .navIcon-message,
.navbar a .navIcon-me {
  position: relative;
  z-index: 0;
  top: .3rem;
}
.navbar .center {
  font-size: 16px;
  font-weight: normal;
}
.navbar:after {
  display: none;
}
.popup-overlay {
  display: none;
}
.toolbar-inner {
  padding: 0;
}
.tabbar a {
  color: #6F00BD;
}
.tabbar-labels a.link {
  padding-top: .1rem;
}
.tabbar a.active {
  background-color: #6F00BD;
  color: #fff;
}
.button {
  border-color: #6F00BD;
  color: #6F00BD;
  background-color: white;
}
.button.button-fill {
  background-color: #6F00BD;
  color: white;
}
a.active .navIcon.navIcon-home {
  background: url(../img/icon/home_on.png) no-repeat center center;
  background-size: 100%;
}
a.active .navIcon.navIcon-hall {
  background: url(../img/icon/hall_on.png) no-repeat center center;
  background-size: 100%;
}
a.active .navIcon.navIcon-prize {
  width: 1.1rem;
  background: url(../img/icon/prize_on.png) no-repeat center center;
  background-size: 100%;
}
a.active .navIcon.navIcon-magic {
  width: 1.25rem;
  background: url(../img/icon/magic_on.png) no-repeat center center;
  background-size: 100%;
}
a.active .navIcon.navIcon-me {
  background: url(../img/icon/me_on.png) no-repeat center center;
  background-size: 100%;
}
.modal-preloader {
  width: 180px;
  margin-left: -90px;
}
.modal-preloader .modal-inner {
  padding: .5rem;
}
.modal-preloader .modal-title {
  font-size: 16px;
}
.swiper-container {
  height: 8rem;
}
.swiper-container img {
  width: 100%;
}
.searchIndex.searchbar {
  background: transparent;
}
.searchIndex.searchbar .searchbar-input {
  height: 1.6rem;
  border-radius: 3rem;
  background-color: white;
  opacity: .8;
}
.searchIndex.searchbar input[type=search] {
  border-radius: 0;
  background-color: transparent;
  background-position: 98% center;
  background-size: 36px 16px;
}
