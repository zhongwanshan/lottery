// 统一定义颜色
@mainColor: #6F00BD;
@color1: #e9e9e9;
@color2: #a59faa;
@color3: #9e9e9e;
@color5: #41d494;
@color6: #ff5f5f;
@color7: #e51f42;
@color8: #575cea;
@color9: #9962C0;
@color10: #695BFA;
@colorddd: #dddddd;
@color333: #333333;
@colorF1: #f1f1f1;
@colorF3: #F3F3F3;
@colorb2: #b2b2b2; 
@colord8: #d8e8fb;
@colord9: #f3f8fe;
@colortip: #fffaed;
// 统一定义字体
@font12: 12px;
@font13: 13px;
@font14: 14px;
@font16: 16px;
@font18: 18px;
@font20: 20px;
@font24: 24px;
// 统一定义宽,高
@maxWidth: 1000px;
// 覆盖 Flat ui 样式 
.btn-sm {
    padding: 5px 13px;
    font-size: @font13;
}

.center() {
    position: relative;
    z-index: 0;
    width: @maxWidth;
    margin: 0 auto;
}

.clear {
    display: block;
    clear: both;
}

body {
    font-family: "微软雅黑", sans-serif;
    background-color: @colorF3;
}

table {
    border-collapse: collapse;
}

a {
    cursor: pointer;
}

// 工具类
.circle {
    position: relative;
    z-index: 0;
    display: inline-block;
    border-radius: 50%;
    width: 30px;
    height: 30px;
    line-height: 30px;
    margin: 0;
    color: @color7;
    text-align: center;
    font-size: @font14;
    cursor: pointer;
    overflow: hidden;
    background: radial-gradient(circle at 50% 120%, #D2D2D2, #EFEFEF 10%, #ECEAEA 80%, #D0CECE 100%);
    margin-right: 5px;
    input {
        position: absolute;
        z-index: 10;
        border-radius: 50%;
        width: 30px;
        height: 30px;
        line-height: 30px;
        opacity: 0;
        left: 0;
        cursor: pointer;
    }
    &.blueBall {
        color: @color10;
    }
    &.active {
        &.redBall {
            color: white;
            background: radial-gradient(circle at 50% 120%, #F99D9D, #E69898 10%, #EA4F4F 80%, #E8A2A2 100%);
        }
        &.blueBall {
            color: white;
            background: radial-gradient(circle at 50% 120%, #2761B7, #85ACE8 10%, #075CDC 80%, #1950A2 100%);
        }
    }
}

// logo 组件
.c-logo {
    .img {
        width: 170px;
        height: 60px;
        margin: 0;
        padding: 0;
        text-indent: -100000000rem;
        background: url(/assets/img/logo.png) no-repeat center center;
    }
}

// 搜索组件
.c-search {
    display: none;
    position: relative;
    z-index: 0;
    width: 360px;
    input {
        width: 100%;
        height: 38px;
        padding: 0 30px;
        border: 1px solid @mainColor;
        border-radius: 20px;
        font-size: @font14;
    }
    button {
        position: absolute;
        z-index: 100;
        right: 0;
        top: 0;
        width: 60px;
        height: 38px;
        text-align: center;
        border-radius: 0 20px 20px 0;
        background-color: @mainColor;
        border: none;
        font-size: @font13;
        color: white;
    }
}

// 用户信息组件
.c-info {
    overflow: hidden;
    border-radius: 0 0 10px 10px;
    padding: 0 15px;
    background-color: @color1;
    a {
        float: left;
        color: @color333;
        padding: 7px;
        font-size: @font13;
        i {
            display: inline-block;
            width: 5px;
            height: 5px;
            margin-right: 5px;
            border-radius: 50%;
            background-color: @mainColor;
        }
    }
}

.c-menu {
    display: none;
    background-color: white;
    border: 1px solid @color9;
    line-height: 1.8;
    .mMenu {
        a {
            display: block;
            padding: 8px 6px;
            text-align: left;
            color: @color333;
            font-size: @font16;
            border-bottom: 1px solid @color3;
            img {
                width: 35px;
                height: 35px;
                margin-right: 10px;
            }
            span {
                padding-left: 10px;
                font-size: @font14;
                color: @color2;
            }
        }
    }
    .menulist {
        padding: 5px 10px;
        table {
            width: 100%;
            .title {
                width: 30px;
                text-align: center;
                vertical-align: middle;
                font-size: @font16;
                background-color: @colorddd;
                padding: 10px 0;
                color: @color333;
            }
            .item {
                padding: 0 10px;
                vertical-align: top;
                &.item-b {
                    a {
                        float: none;
                        display: block;
                        width: 100%;
                    }
                }
                a {
                    float: left;
                    width: 45%;
                    padding: 3px 0;
                    color: @color333;
                    font-size: @font14;
                    text-align: left;
                }
            }
        }
    }
}

// 导航组件
.c-nav {
    height: 50px;
    line-height: 50px;
    .navlist {
        width: @maxWidth;
        margin: 0 auto;
        .navItem {
            float: left;
            height: 50px;
            line-height: 50px;
            padding: 0 30px;
            color: white;
            font-size: @font16;
            &:nth-child(5) {
                display: none;
            }
            &:nth-child(6) {
                display: none;
            }
            &:nth-child(7) {
                display: none;
            }
            &.selectMenu {
                position: relative;
                z-index: 0;
                background-color: @color10;
                width: 150px;
                text-align: center;
                cursor: pointer;
                &:hover {
                    background-color: lighten(@color10, 10%);
                    color: white;
                    .c-menu {
                        a {
                            color: @color333;
                        }
                    }
                }
                .c-menu {
                    position: absolute;
                    z-index: 100;
                    left: 0;
                    top: 50px;
                    width: 259px;
                    height: 360px;
                }
            }
            &:hover,
            &.active {
                background-color: @color8;
                color: white;
            }
        }
    }
}

// 头部模块，包住logo组件，搜索组件，用户信息组件
.header {
    background-color: white;
    .center;
    padding: 5px 0 5px;
    overflow: hidden;
    .c-logo {
        float: left;
    }
    .c-search {
        float: right;
    }
    .c-info {
        position: absolute;
        z-index: 10;
        right: 0;
        top: 0;
    }
}

.headerWrap {
    background-color: white;
}

.navigation {
    position: relative;
    z-index: 100;
    background: url(../img/navLine.png) repeat-x left center;
    .c-nav {
        .center;
    }
}

.bannerWrap {
    height: 360px;
    background-color: @color8;
    .c-banner {
        .center;
        position: relative;
        z-index: 0;
        img {
            width: 100%;
            height: 360px;
        }
    }
}

.c-box {
    // border: 1px solid @color1;
    &.c-box--min {
        .title {
            background-color: @colord8;
            height: 30px;
            line-height: 30px;
            color: @color333;
            a {
                color: @color333;
            }
        }
    }
    .title {
        height: 48px;
        line-height: 48px;
        background-color: @color8;
        color: white;
        font-size: @font14;
        padding-left: 10px;
        a {
            float: right;
            margin-right: 20px;
            color: white;
        }
        .subTab {
            padding-left: 52px;
            ul {
                overflow: hidden;
                li {
                    float: left;
                    list-style: none;
                    cursor: pointer;
                    span {
                        display: block;
                        font-size: @font12;
                        padding: 0 15px;
                    }
                    &.active {
                        background-color: white;
                        color: @color333;
                    }
                }
            }
        }
    }
}

