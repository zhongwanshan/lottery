                }
            }
        } 
    }
}

.tabType {
    overflow: hidden;
    border-bottom: 3px solid @color7;
    padding: 20px 20px 0 20px;
    .sign {
        float: left;
        width: 100px;
        img {
            width: 60px;
            height: 60px;
        }
    }
    .tabbox {
        position: relative;
        z-index: 0;
        left: -20px;
        padding-left: 100px;
        .info {
            overflow: hidden;
            h2 {
                float: left;
                margin: 0;
                font-size: @font20;
            }
            p {
                float: left;
                font-size: @font13;
                color: @color3;
                padding-left: 10px;
                &.tips {
                    strong {
                        color: @color7;
                        padding: 0 5px;
                    }
                    .tipInfo {
                        text-align: right;
                    }
                }
            }
        }
        .tabs {
            position: relative;
            z-index: 0;
            .matchTip {
                position: absolute;
                z-index: 10;
                top: 5px;
                right: 0;
                font-size: @font12;
                color: @color2;
            }
            ul {
                margin: 0;
                padding: 0;
                overflow: hidden;
                border-bottom: none;
                &.longer {
                    li {
