(function(){var e=utility.getQueryParams();new Vue({el:"#js-payQrcode",data:{userInfo:USERINFO},methods:{userLogout:function(){utility.userLogout(function(){window.location.replace(CONFIG.HOST+"/www/index.html")})},getOrderDetail:function(){utility.interactWithServer({headerInfo:{appType:"2",userId:USERINFO?USERINFO.userId:"",userToken:USERINFO?USERINFO.userToken:"",actionUrl:utility.formatActionUrl(CONFIG.SERVICE.walletService,CONFIG.ACTION.queryPay,{action:CONFIG.ACTION.queryPay,orderCode:e.orderCode})},url:CONFIG.HOST+CONFIG.SERVICE.walletService,dataObj:{action:CONFIG.ACTION.queryPay,orderCode:e.orderCode},successCallback:function(e){200==e.code&&(window.location.href=CONFIG.HOST+"/www/view/user/order.html")}})}},created:function(){var t=this;setTimeout(function(){e&&e.qrcode&&$("#js-QRcode").empty().qrcode(decodeURIComponent(e.qrcode))},1e3),setInterval(function(){t.getOrderDetail()},4e3)}})})();